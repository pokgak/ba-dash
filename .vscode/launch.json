    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Dashboard",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/app.py",
            "console": "integratedTerminal",
        },
        {
            "name": "Run Test Server",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "test-server/server.py",
                "FLASK_ENV": "development",
            },
            "args": [
                "run",
            ],
            "console": "integratedTerminal",
        }
    ],
}